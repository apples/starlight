; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Starlight"
run/main_scene="res://scenes/battle_scene/battle_scene.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

Settings="*res://singletons/settings.gd"
FullscreenToggle="*res://singletons/fullscreen_toggle.gd"
ClickTargetManager="*res://singletons/click_target_manager.gd"
CardDatabase="*res://addons/card_engine/card_database.gd"

[debug]

gdscript/warnings/unassigned_variable_op_assign=2
gdscript/warnings/assert_always_true=2
gdscript/warnings/assert_always_false=0

[display]

window/size/viewport_width=1200
window/size/viewport_height=672
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Starlight"

[editor_plugins]

enabled=PackedStringArray("res://addons/card_engine/plugin.cfg", "res://addons/noise_maker/plugin.cfg")

[file_customization]

folder_colors={
"res://data/": "green",
"res://objects/": "red"
}

[importer_defaults]

texture={
"compress/channel_pack": 0,
"compress/hdr_compression": 1,
"compress/high_quality": false,
